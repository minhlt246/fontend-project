extends _layouts/master

block content
    section
        .container.py-5
            .row.g-5.align-items-center
                .col-md-12.col-lg-7
                    h4.mb-3.text-secondary 100% Organic Foods
                    h1.mb-5.display-3.text-primary Organic Veggies &amp; Fruits Foods
                    .position-relative.mx-auto
                        input.form-control.border-2.border-secondary.w-75.py-3.px-4.rounded-pill(type='number' placeholder='Search')
                        button.btn.btn-primary.border-2.border-secondary.py-3.px-4.position-absolute.rounded-pill.text-white.h-100(type='submit' style='top: 0; right: 25%;') Submit Now
                .col-md-12.col-lg-5
                    #carouselId.carousel.slide.position-relative(data-bs-ride='carousel')
                        .carousel-inner(role='listbox')
                            .carousel-item.rounded
                                img.img-fluid.w-100.bg-secondary.rounded(src='client/images/featured/feature-1.jpeg' alt='First slide')
                                a.btn.px-4.py-2.text-white.rounded(href='#') T-Shirt
                            .carousel-item.rounded.active
                                img.img-fluid.w-100.bg-secondary.rounded(src='client/images/featured/feature-1.jpeg' alt='Second slide')
                                a.btn.px-4.py-2.text-white.rounded(href='#') Bland
                            .carousel-item.rounded
                                img.img-fluid.w-100.bg-secondary.rounded(src='client/images/featured/feature-1.jpeg' alt='three slide')
                                a.btn.px-4.py-2.text-white.rounded(href='#') Bell
                            .carousel-item.rounded
                                img.img-fluid.w-100.bg-secondary.rounded(src='client/images/featured/feature-1.jpeg' alt='for slide')
                                a.btn.px-4.py-2.text-white.rounded(href='#') Shose
                        button.carousel-control-prev(type='button' data-bs-target='#carouselId' data-bs-slide='prev')
                            span.carousel-control-prev-icon(aria-hidden='true')
                            span.visually-hidden Previous
                        button.carousel-control-next(type='button' data-bs-target='#carouselId' data-bs-slide='next')
                            span.carousel-control-next-icon(aria-hidden='true')
                            span.visually-hidden Next
        #product.product.product-box.pb-4.my-5
            .product-content.pb-3.pb-lg-5
                .container
                    .wow.fadeInUp(data-wow-delay='0.3s' data-wow-duration='0.3s')
                        .justify-content-center.my-5.w-100
                            .text-center.justify-content-center.mb-2
                                .page-title
                                    h1.text-center.service-title.text-uppercase.color-titlel Sản Phẩm Hot
                                    .fs-5.text-center.service-title.text-uppercase.text-while.justify-content-center
                                         |   Meet of Professionals
                    .row
                        - for (var i = 0; i < 8; ++i)
                            .col-md-6.col-lg-3.wow.fadeInUp.my-2(data-wow-delay='0.5s' data-wow-duration='0.3s')
                                .card.product-card.hvr-float
                                    .card-body
                                        .card-img.justify-content-center.team-banner
                                            img.product-img(src="client/images/product-"+ i + ".jpeg" alt="Product Image")

                                        .mx-3.product-bg
                                            img.team-people(src='client/images/VietNam.png' alt="Country Flag")

                                        .pt-2.text-start.d-flex.align-items-center
                                            i.fa-solid.fa-user
                                            p.mb-0.mx-3 55K Buying

                                        .d-flex.align-items-center.justify-content-around
                                            h5#rating-display.mt-2.text-center.mb-0 Rating: 0
                                            .star-rating.d-flex.justify-content-center.pt-2
                                                span.star(data-value='5') &#9733;
                                                span.star(data-value='4') &#9733;
                                                span.star(data-value='3') &#9733;
                                                span.star(data-value='2') &#9733;
                                                span.star(data-value='1') &#9733;

                                        .d-flex.align-items-center.mt-1
                                            i.fa-solid.fa-location-dot
                                            p.mb-0.ms-3 VietNam

                                    .product-team.d-flex.text-start.my-2
                                        h4.text-highline.ms-3.mb-0 $1,400
                                        p.ms-3.text-decoration-line-through.mb-0 $1,200

                                    p.text-start.mx-3
                                        | Là một trong số những loại áo có cổ, hay còn được nhiều người biết tới là áo cổ gập

                                    .align-items-center.text-start.mx-3.mb-2
                                        a.btn.btn-submit.text-white(type='button' href='detail.html') Order Now

        #sell.featured.spad
                .container
                    .row
                        .col-lg-12
                            .text-center.mb-3.text-highline
                                h2 Featured Product
                            .featured-controls
                                ul.controls-list
                                    li.active.list-nav(data-filter='*') All
                                    li.list-nav(data-filter='. ') Nhẫn
                                    li.list-nav(data-filter='.') Dây Chuyền
                                    li.list-nav(data-filter='.') Lăc tay
                                    li.list-nav(data-filter='.') Dây Lưng
                    .featured-filter
                        .row
                            - for (var i = 1; i < 9; ++i)
                                .col-lg-3.col-md-4.col-sm-6
                                    .featured-item.mb-4
                                        .featured-item-pic.set-bg(data-setbg)
                                            img.w-100(src="client/images/featured/feature-"+ i + ".jpg")
                                            ul.featured-item-pic-hover
                                                li.featured-item-list
                                                    a(href='#')
                                                        i.fa.fa-heart
                                                li.featured-item-list
                                                    a(href='#')
                                                        i.fa.fa-retweet
                                                li.featured-item-list
                                                    a(href='#')
                                                        i.fa.fa-shopping-cart
                                        .featured-item-text.text-highline.text-center
                                            h6.text
                                                a(href='#') Crab Pool Security
                                            h5.text-price $30.00
        #blogsection.from-blog.spad
                .container
                    .row
                        .col-lg-12
                            .section-title.from-blog-title.text-center.mb-4.text-highline
                                h2 From The Blog
                    .row
                        .col-lg-4.col-md-4.col-sm-6
                            .blog-item
                                .blog-item-pic
                                    img.w-100(src='client/images/blog/blog-1.png' alt='')
                                .blog-item-text
                                    .d-flex.text-start
                                        p.mb-0
                                            i.fa-regular.fa-calendar
                                            |  May 4,2019
                                        p.mb-0.ms-3
                                            i.fa-regular.fa-comment
                                            |  5
